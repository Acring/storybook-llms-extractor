{
  "name": "@acring/storybook-llms-extractor",
  "description": "A CLI tool that extracts documentation from Storybook builds and converts it to LLM-friendly formats.",
  "version": "0.2.2",
  "type": "commonjs",
  "bin": {
    "storybook-llms-extractor": "./bin/storybook-llms-extractor.js"
  },
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "*.md",
    "dist/**/*",
    "bin/**/*"
  ],
  "dependencies": {
    "@swc/helpers": "^0.5.1",
    "playwright": "^1.49.1",
    "tslib": "^2.7.0",
    "turndown": "^7.2.0",
    "turndown-plugin-gfm": "^1.0.2",
    "yargs": "^17.7.2"
  },
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/turndown": "^5.0.5",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.0.0"
  }
}
